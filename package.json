    "game"
  ],
  "author": "Gregg Tavares",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greggman/hft-boomboom/issues"
  },
  "homepage": "https://github.com/greggman/hft-boomboom#readme",
  "dependencies": {
    "happyfuntimes": "^0.2.1",
    "hft-game-utils": "^1.0.0",
    "hft-sample-ui": "^1.0.0",
    "optionator": "^0.8.2",
    "requirejs": "^2.3.2"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "electron": "^1.4.5",
    "electron-builder": "^8.6.0"
