{
  "name": "hft-boomboom",
  "productName": "BoomBoom",
  "description": "A Massively local multiplayer game with bombs",
  "version": "1.0.0",
  "build": {
    "appId": "com.greggman.hftBoomBoom",
    "asar": false,
    "mac": {
      "category": "public.app-category.games"
    }
  },
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron main.js",
    "startp": "cross-env NODE_ENV=production electron main.js",
